    print(f\"ì´ ìˆ˜ìµë¥      : {total_return:+.2f}%\")\n","            print(f\"í˜„ì¬ BTC ë³´ìœ  : {strategy.position:.4f} BTC\")\n","            print(f\"í˜„ê¸ˆ ì”ê³      : {strategy.cash:,.0f} KRW\")\n","            print(f\"ì´ ê±°ë˜ íšŸìˆ˜  : {len(strategy.transaction_history)}\")\n","\n","        # ê²°ê³¼ ì‹œê°í™”\n","        print(\"\\nğŸ“Š ê²°ê³¼ ì°¨íŠ¸ ìƒì„± ì¤‘...\")\n","        visualize_results(df_with_signals, portfolio_results, factors)\n","\n","    print(\"\\n\" + \"=\" * 60)\n","    print(\"í”„ë¡œê·¸ë¨ ì‹¤í–‰ ì™„ë£Œ!\")\n","    print(\"ì‹¤ì œ ê±°ë˜ë¥¼ ìœ„í•´ì„œëŠ” ë³¸ì¸ì˜ Korbit API í‚¤ë¥¼ ì„¤ì •í•˜ê³ \")\n","    print(\"DEMO_MODEë¥¼ Falseë¡œ ë³€ê²½í•˜ì„¸ìš”.\")\n","    print(\"=\" * 60)\n","\n","# ì‹¤í–‰\n","if __name__ == \"__main__\":\n","    main()"],"metadata":{"id":"rg-ggeW691Cp"},"execution_count":null,"outputs":[]}]}